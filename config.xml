<?xml version='1.0' encoding='UTF-8'?>
<hudson>
  <disabledAdministrativeMonitors/>
  <version>1.437</version>
  <numExecutors>2</numExecutors>
  <mode>NORMAL</mode>
  <useSecurity>true</useSecurity>
  <authorizationStrategy class="com.michelin.cio.hudson.plugins.rolestrategy.RoleBasedAuthorizationStrategy">
    <roleMap type="globalRoles">
      <role name="admin" pattern=".*">
        <permissions>
          <permission>hudson.model.Computer.Create</permission>
          <permission>hudson.model.Computer.Delete</permission>
          <permission>hudson.model.Item.Build</permission>
          <permission>hudson.model.Item.Delete</permission>
          <permission>hudson.model.Item.Create</permission>
          <permission>hudson.model.Computer.Configure</permission>
          <permission>hudson.model.Item.Workspace</permission>
          <permission>hudson.model.Item.Read</permission>
          <permission>hudson.model.View.Configure</permission>
          <permission>hudson.model.Hudson.Read</permission>
          <permission>hudson.model.Hudson.Administer</permission>
          <permission>hudson.scm.SCM.Tag</permission>
          <permission>hudson.model.View.Delete</permission>
          <permission>hudson.model.View.Create</permission>
          <permission>hudson.model.Item.Configure</permission>
          <permission>hudson.model.Run.Delete</permission>
          <permission>hudson.model.Hudson.RunScripts</permission>
          <permission>hudson.model.Run.Update</permission>
        </permissions>
        <assignedSIDs>
          <sid>metson</sid>
          <sid>sfoulkes</sid>
          <sid>meloam</sid>
          <sid>swakef</sid>
        </assignedSIDs>
      </role>
      <role name="anonymous" pattern=".*">
        <permissions>
          <permission>hudson.model.Item.Read</permission>
          <permission>hudson.model.Hudson.Read</permission>
        </permissions>
        <assignedSIDs>
          <sid>anonymous</sid>
        </assignedSIDs>
      </role>
      <role name="developer" pattern=".*">
        <permissions>
          <permission>hudson.model.Item.Build</permission>
          <permission>hudson.model.Item.Delete</permission>
          <permission>hudson.model.Item.Create</permission>
          <permission>hudson.model.View.Delete</permission>
          <permission>hudson.model.View.Create</permission>
          <permission>hudson.model.Item.Read</permission>
          <permission>hudson.model.View.Configure</permission>
          <permission>hudson.scm.SCM.Tag</permission>
          <permission>hudson.model.Hudson.Read</permission>
          <permission>hudson.model.Run.Delete</permission>
          <permission>hudson.model.Hudson.RunScripts</permission>
          <permission>hudson.model.Run.Update</permission>
        </permissions>
        <assignedSIDs>
          <sid>mnorman</sid>
          <sid>maxa</sid>
          <sid>mcinquil</sid>
          <sid>hufnagel</sid>
          <sid>melo</sid>
          <sid>lat</sid>
        </assignedSIDs>
      </role>
    </roleMap>
  </authorizationStrategy>
  <securityRealm class="hudson.security.HudsonPrivateSecurityRealm">
    <disableSignup>false</disableSignup>
    <enableCaptcha>false</enableCaptcha>
  </securityRealm>
  <workspaceDir>${ITEM_ROOTDIR}/workspace</workspaceDir>
  <buildsDir>${ITEM_ROOTDIR}/builds</buildsDir>
  <markupFormatter class="hudson.markup.RawHtmlMarkupFormatter">
    <disableSyntaxHighlighting>false</disableSyntaxHighlighting>
  </markupFormatter>
  <jdks/>
  <viewsTabBar class="hudson.views.DefaultViewsTabBar"/>
  <myViewsTabBar class="hudson.views.DefaultMyViewsTabBar"/>
  <clouds/>
  <slaves>
    <slave>
      <name>workqueue-slave</name>
      <description>WorkQueue slave</description>
      <remoteFS>/var/lib/jenkins</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>EXCLUSIVE</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.slaves.JNLPLauncher"/>
      <label>workqueue-slaves</label>
      <nodeProperties/>
    </slave>
    <slave>
      <name>reqmgr-slave</name>
      <description>ReqMgr slave machine</description>
      <remoteFS>/var/lib/jenkins</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>NORMAL</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.slaves.JNLPLauncher"/>
      <label>reqmgr-slaves</label>
      <nodeProperties/>
    </slave>
    <slave>
      <name>dmwm-wmcore</name>
      <description>For running wmcore unit tests</description>
      <remoteFS>/var/lib/jenkins</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>NORMAL</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.slaves.JNLPLauncher"/>
      <label>wmcore-unit-test-slaves</label>
      <nodeProperties/>
    </slave>
    <slave>
      <name>dmwm-agent-int</name>
      <description>WMAgent integration instance</description>
      <remoteFS>/var/lib/jenkins</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>NORMAL</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.slaves.JNLPLauncher"/>
      <label>agent-slaves</label>
      <nodeProperties/>
    </slave>
    <slave>
      <name>dmwm-wmcore2</name>
      <description>For running wmcore unit tests</description>
      <remoteFS>/var/lib/jenkins</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>NORMAL</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.slaves.JNLPLauncher"/>
      <label></label>
      <nodeProperties/>
    </slave>
  </slaves>
  <quietPeriod>5</quietPeriod>
  <scmCheckoutRetryCount>0</scmCheckoutRetryCount>
  <views>
    <hudson.model.AllView>
      <owner class="hudson" reference="../../.."/>
      <name>All</name>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
    </hudson.model.AllView>
    <au.com.centrumsystems.hudson.plugin.buildpipeline.BuildPipelineView>
      <owner class="hudson" reference="../../.."/>
      <name>WMAgent test pipeline</name>
      <description>WMAgent deploy, test pipeline</description>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
      <selectedJob>deploy-wmagent</selectedJob>
      <noOfDisplayedBuilds>5</noOfDisplayedBuilds>
      <buildViewTitle>WMAgent test pipeline</buildViewTitle>
      <triggerOnlyLatestJob>false</triggerOnlyLatestJob>
    </au.com.centrumsystems.hudson.plugin.buildpipeline.BuildPipelineView>
    <listView>
      <owner class="hudson" reference="../../.."/>
      <name>Deployment</name>
      <description>Deployment tests</description>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
      <jobNames class="tree-set">
        <comparator class="hudson.util.CaseInsensitiveComparator"/>
      </jobNames>
      <jobFilters/>
      <columns>
        <hudson.views.StatusColumn/>
        <hudson.views.WeatherColumn/>
        <hudson.views.JobColumn/>
        <hudson.views.LastSuccessColumn/>
        <hudson.views.LastFailureColumn/>
        <hudson.views.LastDurationColumn/>
        <hudson.views.BuildButtonColumn/>
        <de.fspengler.hudson.pview.ConsoleViewColumn/>
      </columns>
      <includeRegex>.*deploy.*</includeRegex>
    </listView>
    <au.com.centrumsystems.hudson.plugin.buildpipeline.BuildPipelineView>
      <owner class="hudson" reference="../../.."/>
      <name>Patch queue</name>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
      <selectedJob>WMCore-py2.6-mysql-roundabout</selectedJob>
      <noOfDisplayedBuilds>10</noOfDisplayedBuilds>
      <buildViewTitle>Patch Queue</buildViewTitle>
      <triggerOnlyLatestJob>false</triggerOnlyLatestJob>
    </au.com.centrumsystems.hudson.plugin.buildpipeline.BuildPipelineView>
    <au.com.centrumsystems.hudson.plugin.buildpipeline.BuildPipelineView>
      <owner class="hudson" reference="../../.."/>
      <name>Full integration chain</name>
      <description>Full integration chain</description>
      <filterExecutors>true</filterExecutors>
      <filterQueue>true</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
      <selectedJob>deploy-reqmgr</selectedJob>
      <noOfDisplayedBuilds>5</noOfDisplayedBuilds>
      <buildViewTitle>Full integration chain</buildViewTitle>
      <triggerOnlyLatestJob>false</triggerOnlyLatestJob>
    </au.com.centrumsystems.hudson.plugin.buildpipeline.BuildPipelineView>
    <maps.hudson.plugin.xfpanel.XFPanelView>
      <owner class="hudson" reference="../../.."/>
      <name>Summary view</name>
      <description>Summary view</description>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
      <jobNames class="tree-set">
        <comparator class="hudson.util.CaseInsensitiveComparator"/>
        <string>WMCore-py2.6-mysql</string>
      </jobNames>
      <jobFilters/>
      <columns/>
      <colors>
        <okBG>#7E7EFF</okBG>
        <okFG>#FFFFFF</okFG>
        <failedBG>#FFC130</failedBG>
        <failedFG>#FFFFFF</failedFG>
        <brokenBG>#FF0000</brokenBG>
        <brokenFG>#FFFFFF</brokenFG>
        <otherBG>#CCCCCC</otherBG>
        <otherFG>#FFFFFF</otherFG>
      </colors>
      <numColumns>1</numColumns>
      <refresh>300</refresh>
      <fullHD>true</fullHD>
      <showDescription>false</showDescription>
      <showZeroTestCounts>false</showZeroTestCounts>
      <sortDescending>true</sortDescending>
    </maps.hudson.plugin.xfpanel.XFPanelView>
  </views>
  <primaryView>All</primaryView>
  <slaveAgentPort>44475</slaveAgentPort>
  <label></label>
  <crumbIssuer class="hudson.security.csrf.DefaultCrumbIssuer">
    <excludeClientIPFromCrumb>false</excludeClientIPFromCrumb>
    <PROXY__HEADER>X-Forwarded-For</PROXY__HEADER>
  </crumbIssuer>
  <nodeProperties/>
  <globalNodeProperties/>
</hudson>